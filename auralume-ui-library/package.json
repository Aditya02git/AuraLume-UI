{
  "name": "aura-lume1",
  "version": "1.0.8",
  "description": "A React UI component library built with JavaScript and Vite",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/aura-lume1.css": "./dist/aura-lume1.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.demo.config.js",
    "build": "vite build",
    "preview": "vite preview --config vite.demo.config.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "library",
    "javascript"
  ],
  "author": "Aditya Mondal",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@react-three/drei": "^9.0.0",
    "@react-three/fiber": "^8.15.16",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "three": "^0.180.0",
    "vite": "^7.1.4"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "cannon-es": "^0.20.0",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.9.2",
    "simplex-noise": "^4.0.3",
    "tailwindcss": "^4.1.13"
  }
}
